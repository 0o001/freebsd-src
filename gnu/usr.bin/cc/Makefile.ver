BASEVER!=	cat ${GCCDIR}/BASE-VER
DATESTAMP!=	cat ${GCCDIR}/DATESTAMP
DEVPHASE!=	cat ${GCCDIR}/DEV-PHASE

version.o:	version.c
	${CC} ${CFLAGS} -DBASEVER=\"${BASEVER}\" \
		-DDATESTAMP=\"\ ${DATESTAMP}\" \
		-DDEVPHASE=\"\ ${DEVPHASE}\" -c ${.IMPSRC}
