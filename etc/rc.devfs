#
# If there is a global system configuration file, suck it in.
if [ -f /etc/defaults/rc.conf ]; then
	. /etc/defaults/rc.conf
elif [ -f /etc/rc.conf ]; then
	. /etc/rc.conf
fi

# Setup DEVFS, ie permisisons, links etc.

ln -fs /dev/ttyv0 /dev/vga
