#!/bin/sh
#
# $Id: 100.clean-disks,v 1.1.1.1.2.1 1999/03/30 08:58:33 joerg Exp $
#

exit 0		# do not run by default

find / \( ! -fstype local -o -fstype rdonly \) -a -prune -o \
	\( -name '[#,]*' -o -name '.#*' -o -name a.out -o -name '*.core' \
	   -o -name '*.CKP' -o -name '.emacs_[0-9]*' \) \
		-a -atime +3 -delete
