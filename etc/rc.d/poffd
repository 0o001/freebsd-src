# $FreeBSD: src/etc/rc.d/poffd,v 1.2 2002/06/13 22:14:36 gordon Exp $
#

# PROVIDE: poffd
# REQUIRE: DAEMON

. /etc/rc.subr

name="poffd"
rcvar=$name
command="/usr/sbin/${name}"
start_precmd="test -c /dev/pow0"

load_rc_config $name
run_rc_command "$1"
