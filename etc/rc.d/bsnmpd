#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/bsnmpd,v 1.1.2.1 2005/05/19 11:35:15 glebius Exp $
#

# PROVIDE: bsnmpd
# REQUIRE: NETWORKING syslogd
# KEYWORD: nojail

. /etc/rc.subr

name="bsnmpd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
pidfile="/var/run/snmpd.pid"

load_rc_config $name
run_rc_command "$1"
