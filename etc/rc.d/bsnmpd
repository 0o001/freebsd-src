#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/bsnmpd,v 1.1.12.1.2.1 2009/04/15 03:14:26 kensmith Exp $
#

# PROVIDE: bsnmpd
# REQUIRE: NETWORKING syslogd
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="bsnmpd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
pidfile="/var/run/snmpd.pid"

load_rc_config $name
run_rc_command "$1"
