#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/bsnmpd,v 1.1.20.1 2008/11/25 02:59:29 kensmith Exp $
#

# PROVIDE: bsnmpd
# REQUIRE: NETWORKING syslogd
# KEYWORD: nojail

. /etc/rc.subr

name="bsnmpd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
pidfile="/var/run/snmpd.pid"

load_rc_config $name
run_rc_command "$1"
