#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/nfscbd,v 1.1.2.1.4.1 2010/06/14 02:09:06 kensmith Exp $
#

# PROVIDE: nfscbd
# REQUIRE: NETWORKING nfsuserd
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="nfscbd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
sig_stop="USR1"

load_rc_config $name

run_rc_command "$1"
