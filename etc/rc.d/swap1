#!/bin/sh
#
# $NetBSD: swap1,v 1.8 2002/03/24 15:52:41 lukem Exp $
# $FreeBSD: src/etc/rc.d/swap1,v 1.6.2.3 2004/10/16 08:13:19 pjd Exp $
#

# PROVIDE: localswap
# REQUIRE: disks
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="swap1"
stop_precmd='checkyesno swapoff || [ -n "$rc_force" ]'
start_cmd='swapon -a'
stop_cmd='swapoff -a'

load_rc_config swap
run_rc_command "$1"
