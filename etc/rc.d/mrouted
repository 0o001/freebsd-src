#!/bin/sh
#
# $NetBSD: mrouted,v 1.6 2002/03/22 04:33:59 thorpej Exp $
# $FreeBSD: src/etc/rc.d/mrouted,v 1.2 2002/06/13 22:14:36 gordon Exp $
#

# PROVIDE: mrouted
# REQUIRE: DAEMON
# BEFORE: LOGIN
# KEYWORD: FreeBSD NetBSD

. /etc/rc.subr

name="mrouted"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/${name}.conf"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
