#!/bin/sh
#
# $NetBSD: rarpd,v 1.5 2002/03/22 04:33:59 thorpej Exp $
# $FreeBSD: src/etc/rc.d/rarpd,v 1.4.2.1 2004/10/10 09:50:54 mtm Exp $
#

# PROVIDE: rarpd
# REQUIRE: DAEMON
# BEFORE:  LOGIN
# KEYWORD: nojail

. /etc/rc.subr

name="rarpd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/ethers"

load_rc_config $name
run_rc_command "$1"
