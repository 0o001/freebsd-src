#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/kerberos,v 1.4.2.3 2007/10/25 17:20:22 mtm Exp $
#

# PROVIDE: kerberos
# REQUIRE: NETWORKING

. /etc/rc.subr

name="kerberos5"
rcvar="kerberos5_server_enable"
command_args="--detach"

load_rc_config $name
command="${kerberos5_server}"
kerberos5_flags="${kerberos5_server_flags}"
run_rc_command "$1"
