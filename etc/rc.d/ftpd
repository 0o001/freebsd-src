#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/ftpd,v 1.2.2.1.4.1 2010/06/14 02:09:06 kensmith Exp $
#

# PROVIDE: ftpd
# REQUIRE: LOGIN cleanvar
# KEYWORD: shutdown

. /etc/rc.subr

name="ftpd"
rcvar=`set_rcvar`
command="/usr/libexec/${name}"
pidfile="/var/run/${name}.pid"
start_precmd=ftpd_prestart

ftpd_prestart()
{
	rc_flags="-D ${rc_flags}"
	return 0
}

load_rc_config $name
run_rc_command "$1"
