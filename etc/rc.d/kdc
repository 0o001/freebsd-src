#!/bin/sh
#
# $NetBSD: kdc,v 1.5 2002/03/22 04:33:58 thorpej Exp $
# $FreeBSD: src/etc/rc.d/kdc,v 1.2 2002/06/13 22:14:36 gordon Exp $
#

# PROVIDE: kdc
# REQUIRE: NETWORKING
# BEFORE:  SERVERS

. /etc/rc.subr

name="kdc"
rcvar=$name
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/krb5.conf"

load_rc_config $name
run_rc_command "$1"
