#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/kadmind,v 1.4.22.1.6.1 2010/12/21 17:09:25 kensmith Exp $
#

# PROVIDE: kadmin
# REQUIRE: kerberos
# BEFORE: DAEMON

. /etc/rc.subr

name="kadmind5"
load_rc_config $name
rcvar="kadmind5_server_enable"
unset start_cmd
command="${kadmind5_server}"
command_args="&"
required_vars="kerberos5_server_enable"

run_rc_command "$1"
