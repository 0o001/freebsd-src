#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/kadmind,v 1.4.18.1 2008/11/25 02:59:29 kensmith Exp $
#

# PROVIDE: kadmin
# REQUIRE: kerberos
# BEFORE: DAEMON

. /etc/rc.subr

name="kadmind5"
load_rc_config $name
rcvar="kadmind5_server_enable"
unset start_cmd
command="${kadmind5_server}"
command_args="&"
required_vars="kerberos5_server_enable"

run_rc_command "$1"
