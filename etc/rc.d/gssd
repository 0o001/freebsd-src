#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/gssd,v 1.1.2.1.8.1 2012/03/03 06:15:13 kensmith Exp $
#

# PROVIDE: gssd
# REQUIRE: root
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="gssd"

load_rc_config $name
rcvar="gssd_enable"
command="${gssd:-/usr/sbin/${name}}"
eval ${name}_flags=\"${gssd_flags}\"
run_rc_command "$1"
