# $FreeBSD: src/tools/tools/nanobsd/Files/root/save_sshkeys,v 1.1.2.1 2005/05/20 09:54:23 phk Exp $

set -ex

mount /dev/ad0s3 /mnt
mkdir -p /mnt/ssh
(
cd /etc/ssh
cp ssh_host_* /mnt/ssh
)
umount /mnt
