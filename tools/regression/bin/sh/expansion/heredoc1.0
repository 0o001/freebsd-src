# $FreeBSD: src/tools/regression/bin/sh/expansion/heredoc1.0,v 1.1.2.2.2.1 2012/03/03 06:15:13 kensmith Exp $

f() { return $1; }

[ `f 42; { cat; } <<EOF
$?
EOF
` = 42 ] || echo compound command bad

[ `f 42; (cat) <<EOF
$?
EOF
` = 42 ] || echo subshell bad

long=`printf %08192d 0`

[ `f 42; { cat; } <<EOF
$long.$?
EOF
` = $long.42 ] || echo long compound command bad

[ `f 42; (cat) <<EOF
$long.$?
EOF
` = $long.42 ] || echo long subshell bad
