# Check that bogus PWD values are not accepted from the environment.

cd / || exit 3
failures=0
[ "$(PWD=foo sh -c 'pwd')" = / ] || : $((failures += 1))
[ "$(PWD=/var/empty sh -c 'pwd')" = / ] || : $((failures += 1))
[ "$(PWD=/var/empty/foo sh -c 'pwd')" = / ] || : $((failures += 1))
[ "$(PWD=/bin/ls sh -c 'pwd')" = / ] || : $((failures += 1))

exit $((failures != 0))
